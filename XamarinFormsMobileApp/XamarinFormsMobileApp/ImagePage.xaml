<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:XamarinFormsMobileApp.MarkupExtensions"
             Title="Image Page"
             x:Class="XamarinFormsMobileApp.ImagePage">
    <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
        <!--<Image Source="https://i0.wp.com/semihcelikol.com/wp-content/uploads/2017/10/XamarinLogo.png?fit=600%2C251&amp;ssl=1"/>-->
        <Image x:Name="myImage" Aspect="Fill"/>
        <Image x:Name="myImageCat1" Aspect="Fill"/>
        <Image x:Name="eleph1" Aspect="AspectFit" Source="{local:EmbeddedImage ResourceId=XamarinFormsMobileApp.Images.eleph1.jpg}"/>
   
        <Image x:Name="myImageDog1" Aspect="AspectFill"/> </StackLayout>
</ContentPage>